# Test Environment Configuration
# Copy this to .env.test and fill in with your TEST Supabase project credentials
# ⚠️ WARNING: Use a TEST project only! Tests may create/delete data.

# Supabase Project Configuration
SUPABASE_PROJECT_ID=your-test-project-id
SUPABASE_URL=https://your-test-project.supabase.co
SUPABASE_ANON_KEY=your-test-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-test-service-role-key

# Database Configuration
SUPABASE_DB_HOST=db.your-test-project.supabase.co
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=postgres
SUPABASE_DB_USER=postgres
SUPABASE_DB_PASSWORD=your-test-db-password

# Test-specific settings
BACKUP_DIR=./backups/test
TEST_MODE=true
